extends layout

block content
  fieldset
    legend
      strong MD5 Generator
    p
      em Выберите файл, md5-хэш которого хотите получить и нажмите отправить
    form(action="", method="post" enctype="multipart/form-data")
      p
        input(type="file" name="file" required)
      p
        input(type="submit" value="Отправить")
